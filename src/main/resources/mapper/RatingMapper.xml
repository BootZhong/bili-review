<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.zfg.learn.dao.RatingMapper">
    <insert id="insertRating" parameterType="com.zfg.learn.model.po.Rating">
        insert into rating
        (media_id, score, count)
        values(#{media_id}, #{score}, #{count})
    </insert>
</mapper>


